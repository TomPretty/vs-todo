{
  "name": "vs-todo",
  "displayName": "vs-todo",
  "description": "Todo lists in vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vs-todo.open",
    "onCommand:vs-todo.createTodo",
    "onCommand:vs-todo.completeTodo"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vs-todo.open",
        "category": "Todo",
        "title": "Open"
      },
      {
        "command": "vs-todo.createTodo",
        "category": "Todo",
        "title": "Create Todo"
      },
      {
        "command": "vs-todo.completeTodo",
        "category": "Todo",
        "title": "Complete Todo"
      }
    ]
  },
  "scripts": {
    "lint": "eslint src --ext ts",
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "jest": "jest"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.21",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "mocha": "^8.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "vscode-test": "^1.4.0",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0"
  }
}
